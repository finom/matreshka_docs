{
  "name": "matreshka.io",
  "private": true,
  "version": "2.0.0",
  "description": "Matreshka.js website",
  "license": "MIT",
  "scripts": {
    "clear": "rimraf dist html/*/temp",
    "test": "echo 'No test specified'",
    "build": "npm run clear && webpack --config webpack/webpack.config.js",
    "watch": "PORT=8110 NODE_ENV=development npm run build -- --watch",
    "serve-dist": "http-server dist -o -s -d -p 8110",
    "deploy": "pm2 deploy ecosystem.json production --force",
    "develop": "npm-run-all --parallel watch serve-dist",
    "upgrade": "ncu -u -a && npm install"
  },
